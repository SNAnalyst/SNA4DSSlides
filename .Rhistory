install.packages("arm")
set.seed(42)
n <- 200
x <- rnorm(n)
y <- rbinom(n, size = 1, prob = 1 / (1 + exp(-(0.5 + 2*x))))
# Logistic regression
fit <- glm(y ~ x, family = binomial)
# Predictions and residuals
yhat <- fitted(fit)
resid <- residuals(fit, type = "pearson")
# Binned residual plot
arm::binnedplot(x = yhat,
y = resid,
nclass = 10,
xlab = "Predicted probability",
ylab = "Pearson residuals",
main = "Binned Residual Plot (arm::binnedplot)")
